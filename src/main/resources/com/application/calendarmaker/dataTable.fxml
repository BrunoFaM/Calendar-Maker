<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.StackPane?>


<StackPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.application.calendarmaker.DataTableController">
   <children>
      <TableView fx:id="myTable" prefHeight="200.0" prefWidth="200.0">
        <columns>
          <TableColumn fx:id="nameCol" prefWidth="75.0" text="Nombre" />
          <TableColumn fx:id="hoursCol" prefWidth="75.0" text="Horas Trabajadas" />
            <TableColumn fx:id="restCol" prefWidth="75.0" text="Descansos" />
            <TableColumn fx:id="halfRestCol" prefWidth="75.0" text="Medio Descansos" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </children>
</StackPane>
